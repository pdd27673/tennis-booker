# Frontend Environment Variables for Local Development
# Copy this file to .env.local and update the values

# API Configuration
VITE_API_URL=http://localhost:8080
VITE_API_TIMEOUT=30000

# Application Settings
VITE_APP_NAME=Tennis Booker
VITE_APP_VERSION=0.1.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_FEATURE_ANALYTICS_ENABLED=true
VITE_FEATURE_NOTIFICATIONS_ENABLED=true
VITE_FEATURE_ADVANCED_SEARCH_ENABLED=true
VITE_FEATURE_DARK_MODE_ENABLED=true
VITE_MOCK_API_ENABLED=false
VITE_DEBUG_MODE=true

# Logging
VITE_LOG_LEVEL=debug

# External Services (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Notes:
# - The backend should be running on localhost:8080 for the API calls to work
# - Set VITE_MOCK_API_ENABLED=true to use mock data instead of real API calls
# - All VITE_ prefixed variables are available in the frontend code via import.meta.env 